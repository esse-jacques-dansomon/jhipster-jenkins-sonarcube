pipeline {
       agent none
        stages {

          stage("Show info") {
            agent any
            steps {
                echo "Hello WTF world"
            }
          }
          stage("build & SonarQube analysis") {
            agent any
            steps {
              withSonarQubeEnv('sq1') {
                sh './mvnw clean package sonar:sonar'
              }
            }
          }

          stage("Quality Gate") {
            steps {
              timeout(time: 1, unit: 'HOURS') {
                waitForQualityGate abortPipeline: true
              }
            }
          }
     }
}
